{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  },
  "buildCommand": "pnpm run vercel-build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "devCommand": "next dev"
}